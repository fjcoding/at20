{
  "name": "at20",
  "version": "1.0.0",
  "description": "Coding (Dev Fundamentals) repository for AT20 group",
  "type": "module",
  "jest": {
    "projects": [
      "katas"
    ],
    "verbose": true,
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ]
  },
  "scripts": {
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:katas": "jest katas",
    "test:checkers": "jest --passWithNoTests src/checkers",
    "test:nmm": "jest --passWithNoTests src/nmm",
    "test:reversi": "jest --passWithNoTests src/reversi"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fjcoding/at20.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fjcoding/at20/issues"
  },
  "homepage": "https://github.com/fjcoding/at20#readme",
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.18.2",
    "eslint": "^8.32.0",
    "eslint-plugin-jest": "^26.4.6",
    "jest": "^28.1.0"
  },
  "dependencies": {
    "prompt-sync": "^4.2.0",
    "prompt-sync-history": "^1.0.1"
  }
}
